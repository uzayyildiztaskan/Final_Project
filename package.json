{
  "main": "main.js",
  "devDependencies": {
    "electron": "^29.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-start": "electron .",
    "electron-pack": "electron-packager . --overwrite --platform=win32 --arch=x64 --icon=./public/icon.ico --prune=true --out=release-builds",
    "electron-pack-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./public/icon.icns --prune=true --out=release-builds",
    "electron-pack-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=./public/icon.png --prune=true --out=release-builds"
  }
}


